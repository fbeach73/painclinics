{
  "name": "agentic-coding-starter-kit",
  "version": "1.1.2",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "pnpm run db:migrate && next build",
    "build:ci": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "npx tsx scripts/setup.ts",
    "env:check": "node -e \"require('./src/lib/env.ts').checkEnv()\" || echo 'Run with tsx: npx tsx -e \"import { checkEnv } from './src/lib/env'; checkEnv();\"'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:dev": "drizzle-kit push",
    "db:reset": "drizzle-kit drop && drizzle-kit push",
    "sync-template": "node create-agentic-app/scripts/sync-templates.js"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.106",
    "@hookform/resolvers": "^5.2.2",
    "@openrouter/ai-sdk-provider": "^1.3.0",
    "@paralleldrive/cuid2": "^3.0.4",
    "@polar-sh/better-auth": "^1.6.0",
    "@polar-sh/sdk": "^0.41.5",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@vercel/blob": "^2.0.0",
    "ai": "^5.0.106",
    "better-auth": "^1.4.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.539.0",
    "mailgun.js": "^12.4.0",
    "mapbox-gl": "^3.17.0",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "react-map-gl": "^8.1.0",
    "react-markdown": "^10.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "latest",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20.19.25",
    "@types/papaparse": "^5.5.1",
    "@types/pg": "^8.15.6",
    "@types/react": "19.2.5",
    "@types/react-dom": "19.2.3",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "shadcn": "^3.5.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
